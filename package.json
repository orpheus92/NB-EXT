{
  "name": "NB-EXT",
  "version": "0.1.0",
  "description": "Notebook Extension for Regulus ",
  "author": "orpheus92",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.11.1",
    "@jupyterlab/apputils": "^0.11.1",
    "@jupyterlab/docregistry": "^0.11.1",
    "@jupyterlab/notebook": "^0.11.1",
    "@phosphor/disposable": "^1.1.2",
    "@orpheus92/jupyterlab_xkcd": "v0.1.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.0"
  }
}
